{
  "name": "@tastiest-io/tastiest-utils",
  "author": "vincentbavitz",
  "description": "Shared typings and utilities for Tastiest",
  "license": "TASTIEST TECHNOLOGIES",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": "git://github.com:tastiest-io/tastiest-utils",
  "version": "1.83.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=12.13"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "NODE_ENV=production yarn test && tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "publish:minor": "npm version minor && npm publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "module": "dist/tastiest-utils.esm.js",
  "size-limit": [
    {
      "path": "dist/tastiest-utils.cjs.production.min.js",
      "limit": "22 KB"
    },
    {
      "path": "dist/tastiest-utils.esm.js",
      "limit": "22 KB"
    }
  ],
  "dependencies": {
    "@contentful/rich-text-types": "^14.1.2",
    "@shopify/shopify-api": "^1.4.0",
    "@stripe/stripe-js": "^1.13.2",
    "@tastiest-io/tastiest-horus": "^0.18.0",
    "@types/react": "^17.0.3",
    "contentful": "^8.2.1",
    "crypto-hash": "^1.3.0",
    "eslint-config-prettier": "^8.5.0",
    "firebase-admin": "^9.6.0",
    "luxon": "^2.0.2",
    "moment": "^2.29.1",
    "stripe": "^8.143.0"
  },
  "peerDependencies": {
    "firebase": "^8",
    "react": "^17.0.2",
    "swr": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@size-limit/preset-small-lib": "^4.9.2",
    "@types/luxon": "^2.0.5",
    "@types/stripe": "^8.0.417",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "autoprefixer": "^9",
    "babel-loader": "^8.2.2",
    "clsx": "^1.1.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.22.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "firebase": "^8.2.1",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "postcss": "^7",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-is": "^17.0.1",
    "size-limit": "^4.9.2",
    "swr": "^1.1.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  }
}
